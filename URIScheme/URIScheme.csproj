<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net4.8;netcoreapp3.1;netstandard2.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>
		<Copyright>Copyright Â© 2021 Trung Nguyen</Copyright>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>trungnt2910</Authors>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/trungnt2910/URIScheme</PackageProjectUrl>
		<RepositoryUrl>https://github.com/trungnt2910/URIScheme</RepositoryUrl>
		<PackageTags>URI scheme manager</PackageTags>
		<Version>1.0.0</Version>
		<PackageId>URIScheme</PackageId>
		<Description>A cross platform solution to register your application to a URI scheme.</Description>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
	</ItemGroup>

</Project>